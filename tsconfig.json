{
	"compilerOptions": {
		"strict": true,
		"checkJs": true,
		"noImplicitAny": true,
		"skipLibCheck": true,
		"noEmit": true,
		"module": "nodenext",
		"target": "es2020",
		"verbatimModuleSyntax": true,
		"moduleResolution": "nodenext",
		"allowImportingTsExtensions": true,
		"stripInternal": true,
		"declaration": true,
		"types": ["bun-types"]
	},
	"include": ["src"]
}
